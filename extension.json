{
	"name": "Bucket",
	"version": "0.1",
	"author": [
		"Jonathan Lee",
		"TehKittyCat",
		"Mudscape",
		"[https://github.com/jayktaylor Jayden Bailey]"
	],
	"url": "https://github.com/weirdgloop/mediawiki-extensions-Bucket",
	"descriptionmsg": "bucket-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43",
		"extensions": {
			"Scribunto": "*"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Bucket\\": "includes/",
		"MediaWiki\\Extension\\Bucket\\Hooks\\Handlers": "includes/Hooks/Handlers"
	},
	"namespaces": [
			{
				"id": 9592,
				"constant": "NS_BUCKET",
				"name": "Bucket",
				"subpages": false,
				"movable": false,
				"defaultcontentmodel": "json",
				"protection": "editbucket"
			},
			{
				"id": 9593,
				"constant": "NS_BUCKET_TALK",
				"name": "Bucket_talk"
			}
	],
	"Hooks": {
		"LinksUpdateComplete": "main",
		"LoadExtensionSchemaUpdates": "schema",
		"MultiContentSave": "main",
		"PageUndelete": "main",
		"PageUndeleteComplete": "main",
		"ScribuntoExternalLibraries": "main",
		"SidebarBeforeOutput": "main",
		"ArticleFromTitle": "main",
		"PageDelete": "main",
		"PageDeleteComplete": "main",
		"ContentModelCanBeUsedOn": "main",
		"BeforeDisplayNoArticleText": "main",
		"TitleIsAlwaysKnown": "main",
		"AfterImportPage": "main",
		"ParserClearState": "main",
		"ParserLimitReportPrepare": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\Bucket\\Hooks\\Handlers\\GeneralHandler",
			"services": [ "Bucket.BucketDatabase" ]
		},
		"schema": {
			"class": "MediaWiki\\Extension\\Bucket\\Hooks\\Handlers\\SchemaHandler"
		}
	},
	"Actions": {
		"bucket": {
			"class": "MediaWiki\\Extension\\Bucket\\BucketAction",
			"services": [
				"Bucket.BucketDatabase",
				"Bucket.BucketPageHelper"
			]
		}
	},
	"APIModules": {
		"bucket": {
			"class":"MediaWiki\\Extension\\Bucket\\BucketApi",
			"services": [
				"Bucket.BucketDatabase"
			]
		}
	},
	"MessagesDirs": {
		"Bucket": [
			"i18n"
		]
	},
	"SpecialPages": {
		"Bucket": {
			"class": "MediaWiki\\Extension\\Bucket\\SpecialBucket",
			"services": [
				"Bucket.BucketDatabase",
				"Bucket.BucketPageHelper"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Bucket"
	},
	"ResourceModules": {
		"ext.bucket.bucketpage.styles": {
			"styles": "resources/ext.bucket.bucketpage.styles.css"
		}
	},
	"config": {
		"BucketWriteEnabledNamespaces": {
			"value": {
				"0": true,
				"1": false,
				"2": true,
				"3": false,
				"4": true,
				"5": false,
				"6": true,
				"7": false,
				"8": false,
				"9": false,
				"10": false,
				"11": false,
				"12": true,
				"13": false,
				"14": true,
				"15": false
			},
			"merge_strategy": "array_plus"
		},
		"BucketDBuser": {
			"value": null
		},
		"BucketDBpassword": {
			"value": null
		},
		"BucketDBhostname": {
			"value": "%"
		},
		"BucketMaxQueryExecutionTime": {
			"value": 500
		},
		"BucketMaxPageExecutionTime": {
			"value": 10000
		},
		"BucketMaxDataPerPage": {
			"value": 1000000
		}
	},
	"GroupPermissions": {
		"sysop": {
			"editbucket": true
		}
	},
	"ConfigRegistry": {
		"bucket": "GlobalVarConfig::newInstance"
	},
	"ExtensionMessagesFiles": {
		"BucketAlias": "Bucket.i18n.alias.php"
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"manifest_version": 2
}
